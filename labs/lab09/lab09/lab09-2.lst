     1                                  ; ===============================
     2                                  ; Программа: Hello World с отладочной информацией
     3                                  ; ===============================
     4                                  
     5                                  SECTION .data
     6                                      ; Сегмент данных
     7 00000000 48656C6C6F2C2000            msg1:    db "Hello, ",0x0     ; Первая часть сообщения
     8                                      msg1Len: equ $ - msg1         ; Длина первой строки
     9                                  
    10 00000008 776F726C64210A              msg2:    db "world!",0xa      ; Вторая часть сообщения + перевод строки
    11                                      msg2Len: equ $ - msg2         ; Длина второй строки
    12                                  
    13                                  SECTION .text
    14                                      global _start
    15                                  
    16                                  _start:
    17                                      ; ===== ПЕРВЫЙ СИСТЕМНЫЙ ВЫЗОВ =====
    18                                      ; Вывод "Hello, "
    19 00000000 B804000000                  mov eax, 4          ; номер системного вызова sys_write
    20 00000005 BB01000000                  mov ebx, 1          ; файловый дескриптор stdout
    21 0000000A B9[00000000]                mov ecx, msg1       ; указатель на строку "Hello, "
    22 0000000F BA08000000                  mov edx, msg1Len    ; длина строки
    23 00000014 CD80                        int 0x80            ; вызов прерывания
    24                                  
    25                                      ; ===== ВТОРОЙ СИСТЕМНЫЙ ВЫЗОВ =====
    26                                      ; Вывод "world!\n"
    27 00000016 B804000000                  mov eax, 4          ; sys_write
    28 0000001B BB01000000                  mov ebx, 1          ; stdout
    29 00000020 B9[08000000]                mov ecx, msg2       ; указатель на строку "world!"
    30 00000025 BA07000000                  mov edx, msg2Len    ; длина строки
    31 0000002A CD80                        int 0x80            ; вызов прерывания
    32                                  
    33                                      ; ===== ЗАВЕРШЕНИЕ ПРОГРАММЫ =====
    34 0000002C B801000000                  mov eax, 1          ; номер системного вызова sys_exit
    35 00000031 BB00000000                  mov ebx, 0          ; код возврата 0 (успех)
    36 00000036 CD80                        int 0x80            ; вызов прерывания
